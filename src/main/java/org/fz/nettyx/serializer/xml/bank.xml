<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<root namespace="bank">

    <model-mappings>
        <model-mapping value="bank">bankcard</model-mapping>
    </model-mappings>

    <enums>
        <enum ref="bank">
            CCB,
            ICBC,
            PBC,
            BOC
        </enum>
    </enums>

    <models>
        <bankcard>
            <bankname :offset="0" :length="2" :enum="{{bank}}(ordinary)"/>
            <cardnumber :offset="2" :length="10" :type="number(12,0)"/>
        </bankcard>
    </models>
</root>



 

 