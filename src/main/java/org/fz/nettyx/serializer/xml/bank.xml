<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<root namespace="bank">

    <model-mappings>
        <model-mapping value="bank" >{{bankcard}}</model-mapping>
    </model-mappings>

    <enums>
        <enum ref="bank">
            CCB,
            ICBC,
            PBC,
            BOC
        </enum>
    </enums>

    <models>
        <model ref="bankcard" offset-type="absolute">
            <prop name="bankname" offset="0" length="2" type="{{bank}}" match="ordinary"/>
            <prop name="cardnumber" offset="2" length="10" type="number"/>
        </model>
    </models>
</root>



 

 