<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<root namespace="bank">

    <model-mapping>
        <bankcard>0077AA</bankcard>
    </model-mapping>

    <!--枚举只支持ordinary, 如果使用name还会涉及到字符集的问题, 所以为了简化了枚举的匹配,只支持ordinary-->
    <enum>
        <bank>
            CCB,
            ICBC,
            PBC,
            BOC
        </bank>
    </enum>

    <switch>
        <status>
            RB,
            FUK
        </status>
    </switch>

    <model>
        <bankcard>
            <!--枚举只支持ordinary,如果使用name会涉及字符集的问题-->
            <bankname :offset="0" :length="2" :type="enum(bank)"/>
            <cardnumber :offset="2" :length="10" :type="int" :order="le"/>
            <maincourse :offset="12" :length="1" :type="switch(status)"/>
            <address :offset="13" :length="3" :type="string(us-ascii)"/>
        </bankcard>
    </model>
</root>



 

 